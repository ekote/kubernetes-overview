{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "hello-service",
        "labels": {
            "name": "hello-service"
        }
    },
    "spec": {
        "type": "LoadBalancer",
        "ports": [
            {
                "name": "http",
                "port": 80,
                "targetPort": 3000
            }
        ],
        "selector": {
            "name": "hello"
        }
    }
}
